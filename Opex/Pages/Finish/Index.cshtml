@page
@model Opex.Pages.Finish.IndexModel
@{
    ViewData["Title"] = "تاییدیه عضویت";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}
<input type="hidden" id="selectedTab" value="verify2" />
<div class="tab-pane" id="verify2">
    <div class="col-md-12" style="margin-top:105px">
        <h2>تاییدیه عضویت</h2>
        <div class="panel panel-body ">
            @if (Model.Parvande != "فعال(صادره)")
            { 
                <div class="alert alert-warning">
                    <p>
                        پرونده شما در حال بررسی جهت صدور پروانه کسب می باشد.
                    </p>
                </div>
                

            }
            else
            { 
                <div class=" text-center alert alert-info">
                    <h4>
                        <strong>پرونده شما با مشخصات زیر فعال شده است.</strong>
                    </h4>
                </div> 
                <div class="row">
                    @if (Model.Members != null)
                    {
                        <div class=" col-md-12   custyle">
                            <table class="table table-striped custab">
                                <thead>
                                    <tr>
                                        <th class="text-right">ردیف</th>
                                        <th class="text-right">وضعیت پرونده</th>
                                        <th class="text-right">تاریخ صدور</th>
                                        <th class="text-right">تاریخ انقضاء</th>
                                        <th class="text-right">شناسه صنفی</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>
                                            1
                                        </td>
                                        <td>
                                            @Model.Members.وضعیتپرونده
                                        </td>
                                        <td>
                                            @Model.Members.تاریخصدور
                                        </td>
                                        <td>
                                            @Model.Members.تاریخانقضاء
                                        </td>
                                        <td>
                                            @Model.Members.شناسهصنفی
                                        </td>
                                    </tr>
                                     
                                </tbody>
                            </table>
                        </div>
                    }
                </div>
            } 
        </div>

        </div>
</div>

        @section Footer{
            <div class="wizard-footer">
                <div class="pull-right">
                </div>
                <div class="pull-left">
                    <a   asp-page="/Payments/Index" class='btn btn-previous btn-default btn-wd' name='Previous'
                        >مرحله قبلی</a>
                </div>
                <div class="clearfix"></div>
            </div>

        }
        @section Scripts{
            <!--   Core JS Files   -->
            <script src="/js/jquery-2.2.4.min.js" type="text/javascript"></script>
            <script src="/js/jquery-1.10.2.js" type="text/javascript"></script>
            <script src="/js/jquery-1.10.2.min.js" type="text/javascript"></script>
            <script src="/js/bootstrap.min.js" type="text/javascript"></script>
            <script src="/js/jquery.bootstrap.wizard.js" type="text/javascript"></script>
            <script src="/js/register.js" type="text/javascript"></script>
            <script src="~/js/jquery.validate.min.js"></script>
            <script src="~/lib/jquery-validation/dist/additional-methods.min.js"></script>
            <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
            <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
            <!--  Plugin for the Wizard -->
            <script src=" /js/demo.js" type="text/javascript"></script>
            <script src=" /js/paper-bootstrap-wizard.js" type="text/javascript"></script>

            <!--  More information about jquery.validate here: https://jqueryvalidation.org/	 -->
            <script src=" /js/jquery.validate.min.js" type="text/javascript"></script>
            <script>
                $(document).ready(function () {
                    $('#wizard').bootstrapWizard('show', 8);
                });
            </script>
        }

